# multiples levels of nesting with multiple nested procedures
var a;
var b;
var c;

procedure One;
    procedure Two;
        procedure Three;
            begin
                c := b + 1;
                write c        # Should write value of b + 1
            end;

        begin
            b := a + 1;
            call Three
        end;

    begin
        a := 5;
        call Two
    end;

call One.

