# Duplicate of nested procedure

const
   a = 10;
var
   b, c;

procedure Outer;
   procedure Inner;
      write 1;
   procedure Inner; # Error duplicate procedure
      begin
         d := b + c;
         write d
      end;
   begin
      call Inner
   end;

begin
   b := a;
   c := a + 5;
   call Outer
end.