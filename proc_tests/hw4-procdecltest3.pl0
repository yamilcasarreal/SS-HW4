# Undefined variable in nested procedure

const
   a = 10;
var
   b, c;

procedure Outer;
   var
      d;
   procedure Inner;
      begin
         d := e;  # Error: Undefined variable e
         write d
      end;
   begin
      call Inner
   end;

begin
   b := a;
   c := a + 5;
   call Outer
end.
